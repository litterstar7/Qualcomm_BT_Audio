file=curator5

# PIO 4 needs to be pulled down to enable the audio amplifier in the CH084 and CH128 modules
# This is only the initial configuration. The application can change this at runtime

# Assign PIOs to Apps (SS 4)
PioSubsystemAllocationElements = [04 04 14 04 15 04 ]

# Set PIO 4 to be an output
PioDirectionElements = [04 01]

# Set PIO 4 to be low
PioDriveElements = [04 00]

# Configure Chargercomms support
VCHGMessageEnable = 0

# Configure Earbud left or right for Chargercomms
# 0 - Ignore all messages
# 1 - Right Earbud
# 2 - Left Earbud
#VCHGMessageEarbud = 0