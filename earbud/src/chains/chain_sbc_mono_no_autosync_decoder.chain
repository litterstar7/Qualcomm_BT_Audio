<chain name="CHAIN_SBC_MONO_NO_AUTOSYNC_DECODER" id="1" generate_endpoint_roles_enum="False" generate_operator_roles_enum="False">

    <include_header name="../earbud_cap_ids.h"/>
    <include_header name="kymera_chain_roles.h"/>

    <operator name="OPR_LATENCY_BUFFER" id="EB_CAP_ID_SWITCHED_PASSTHROUGH">
        <sink name="IN" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_SBC_DECODER" id="CAP_ID_SBC_DECODER">
        <sink name="IN" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <input sink="OPR_LATENCY_BUFFER.IN" role="EPR_SINK_MEDIA"/>
    <connection source="OPR_LATENCY_BUFFER.OUT" sink="OPR_SBC_DECODER.IN"/>
    <output source="OPR_SBC_DECODER.OUT" role="EPR_SOURCE_DECODED_PCM"/>

</chain>
