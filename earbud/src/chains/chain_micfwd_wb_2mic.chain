<!--Chain for HFP (wideband SCO), used for forwarding SCO to other earbud AND receiving MIC -->

<chain name="CHAIN_MICFWD_WB_2MIC" id="1" generate_operator_roles_enum="False" generate_endpoint_roles_enum="False">

    <include_header name="../earbud_cap_ids.h"/>
    <include_header name="kymera_chain_roles.h"/>

    <operator name="OPR_SCO_RECEIVE" id="CAP_ID_WBS_DEC" priority="medium">
        <sink name="SCO_IN" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_SCO_SEND" id="CAP_ID_WBS_ENC" priority="high">
        <sink name="IN" terminal="0"/>
        <source name="SCO_OUT" terminal="0"/>
    </operator>

    <operator name="OPR_CVC_RECEIVE" id="CAP_ID_CVC_RECEIVE_WB">
        <sink name="IN" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_SCOFWD_SPLITTER" id="CAP_ID_SPLITTER">
        <sink name="IN" terminal="0"/>
        <source name="OUT1" terminal="0"/>
        <source name="OUT2" terminal="1"/>
    </operator>

    <operator name="OPR_SCOFWD_BASIC_PASS" id="CAP_ID_BASIC_PASS">
        <sink name="IN" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_SWITCHED_PASSTHROUGH_CONSUMER" id="EB_CAP_ID_SWITCHED_PASSTHROUGH">
        <sink name="IN" terminal="0"/>
        <source name="FORWARD" terminal="0"/>
    </operator>

    <operator name="OPR_SCOFWD_SEND" id="EB_CAP_ID_ASYNC_WBS_ENC">
        <sink name="IN" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_CVC_SEND" id="CAP_ID_CVCHS2MIC_MONO_SEND_WB">
        <sink name="REFERENCE" terminal="0"/>
        <sink name="IN1" terminal="1"/>
        <sink name="IN2" terminal="2"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_SOURCE_SYNC" id="CAP_ID_SOURCE_SYNC">
        <sink name="IN" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_VOLUME_CONTROL" id="EB_CAP_ID_VOL_CTRL_VOL">
        <sink name="MAIN_IN" terminal="0"/>
        <sink name="AUX_IN" terminal="1"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_MICFWD_RECV" id="EB_CAP_ID_ASYNC_WBS_DEC">
        <sink name="IN" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_MICFWD_RECV_SPC" id="EB_CAP_ID_SWITCHED_PASSTHROUGH">
        <sink name="FORWARDED" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_MICFWD_SPC_SWITCH" id="EB_CAP_ID_SWITCHED_PASSTHROUGH">
        <sink name="LOCAL" terminal="0"/>
        <sink name="REMOTE" terminal="1"/>
        <source name="CHOSEN" terminal="0"/>
    </operator>

    <input sink="OPR_SCO_RECEIVE.SCO_IN" role="EPR_SCO_FROM_AIR"/>
    <input sink="OPR_VOLUME_CONTROL.AUX_IN" role="EPR_VOLUME_AUX"/>
    <input sink="OPR_MICFWD_RECV_SPC.FORWARDED" role="EPR_MICFWD_RX_OTA"/>
	<input sink="OPR_CVC_SEND.REFERENCE" role="EPR_CVC_SEND_REF_IN"/>
	<input sink="OPR_CVC_SEND.IN1" role="EPR_CVC_SEND_IN1"/>
	<input sink="OPR_CVC_SEND.IN2" role="EPR_CVC_SEND_IN2"/>
    <output source="OPR_SCO_SEND.SCO_OUT" role="EPR_SCO_TO_AIR"/>
    <output source="OPR_SWITCHED_PASSTHROUGH_CONSUMER.FORWARD" role="EPR_SCOFWD_TX_OTA"/>
	<output source="OPR_VOLUME_CONTROL.OUT" role="EPR_SCO_VOL_OUT"/>

    <connection source="OPR_SCO_RECEIVE.OUT" sink="OPR_CVC_RECEIVE.IN"/>
    <connection source="OPR_CVC_RECEIVE.OUT" sink="OPR_SOURCE_SYNC.IN"/>
    <connection source="OPR_SOURCE_SYNC.OUT" sink="OPR_SCOFWD_SPLITTER.IN"/>
    <connection source="OPR_SCOFWD_SPLITTER.OUT1"  sink="OPR_VOLUME_CONTROL.MAIN_IN"/>
    <connection source="OPR_SCOFWD_SPLITTER.OUT2"  sink="OPR_SCOFWD_BASIC_PASS.IN"/>
    <connection source="OPR_SCOFWD_BASIC_PASS.OUT"  sink="OPR_SCOFWD_SEND.IN"/>
    <connection source="OPR_SCOFWD_SEND.OUT"  sink="OPR_SWITCHED_PASSTHROUGH_CONSUMER.IN"/>
    <connection source="OPR_CVC_SEND.OUT" sink="OPR_MICFWD_SPC_SWITCH.LOCAL"/>
    <connection source="OPR_MICFWD_RECV_SPC.OUT" sink="OPR_MICFWD_RECV.IN"/>
    <connection source="OPR_MICFWD_RECV.OUT" sink="OPR_MICFWD_SPC_SWITCH.REMOTE"/>
    <connection source="OPR_MICFWD_SPC_SWITCH.CHOSEN" sink="OPR_SCO_SEND.IN"/>
</chain>

